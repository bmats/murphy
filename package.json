{
  "name": "murphy",
  "productName": "Murphy",
  "version": "0.1.0",
  "electronVersion": "0.36.5",
  "description": "Simple backup and restore.",
  "private": true,
  "main": "build/src/main.js",
  "scripts": {
    "postinstall": "tsd install",
    "start": "electron .",
    "test": "gulp test",
    "package": "gulp package",
    "build": "npm run package && npm run build:osx && npm run build:win",
    "build:osx": "electron-builder dist/Murphy-darwin-x64/Murphy.app --platform=osx --out=dist --config=builder.json && mv dist/Murphy.dmg \"dist/Murphy OSX.dmg\"",
    "build:win": "electron-builder dist/Murphy-win32-x64 --platform=win --out=dist --config=builder.json && (cd dist; zip -D \"Murphy Windows.zip\" \"Murphy Setup.exe\")"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmats/murphy.git"
  },
  "author": "Bryce Matsumori",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmats/murphy/issues"
  },
  "homepage": "https://github.com/bmats/murphy#readme",
  "dependencies": {
    "async": "^1.5.0",
    "bluebird": "^3.0.6",
    "filequeue": "^0.5.0",
    "js-yaml": "^3.4.6",
    "lodash": "^4.2.1",
    "material-ui": "^0.14.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-motion": "^0.4.2",
    "react-tap-event-plugin": "^0.2.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "electron-builder": "^2.6.0",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.5",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.1.0",
    "gulp-shell": "^0.5.1",
    "gulp-stylus": "^2.1.1",
    "gulp-typescript": "^2.10.0",
    "gulp-util": "^3.0.7",
    "jasmine": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "mock-fs": "^3.5.0",
    "mockdate": "^1.0.3",
    "rimraf": "^2.5.1",
    "run-sequence": "^1.1.5",
    "stylus": "^0.53.0",
    "tsd": "^0.6.5",
    "typescript": "^1.7.3"
  }
}
